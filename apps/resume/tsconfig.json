{
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "target": "es2017",
    "module": "nodenext",
    "lib": [
      "esnext",
      "dom"
    ]
  }
}